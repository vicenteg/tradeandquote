TAQ_JAR=target/scala-2.10/taq_2.10-1.0.jar
INPUT=$1
OUTPUT=$2
BAR=${3:=60}

if [ -z $INPUT ]; then
	echo "Specify an input file."
	exit 1
fi

/opt/spark-1.5.0-bin-hadoop2.6/bin/spark-submit  \
	--master spark://ip-172-20-10-4.ec2.internal:7077 \
	--packages joda-time:joda-time:2.8.2 \
	--jars $TAQ_JAR \
	$TAQ_JAR "$INPUT" "$OUTPUT" $BAR

